[user]
        name = Siaw Young
        email = lausiawyoung@gmail.com

[credential]
        helper = osxkeychain

[push]
        default = simple

[status]
        showUntrackedFiles = all

[transfer]
        fsckobjects = true

[color]
        ui = true

[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green

[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold

[color "status"]
        added = yellow
        changed = green
        untracked = red
[url "git@github.com:"]
	insteadOf = https://github.com/
